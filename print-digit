import java.util.Scanner;
class Sprit
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int dig;
        while(num!=0)
        {
            dig=num%10;
            System.out.print(dig+" ");
            num=num/10;
        }
    }
}
             
